                        {
                            "listen": "test",
                            "script": {
                                "exec": [
                                    "const coldTime = pm.environment.get('cold_time');",
                                    "const warmTime = pm.response.responseTime;",
                                    "",
                                    "pm.test('Response is 200', function() {",
                                    "    pm.response.to.have.status(200);",
                                    "});",
                                    "",
                                    "pm.test('Cached query is faster', function() {",
                                    "    pm.expect(warmTime).to.be.below(coldTime);",
                                    "});",
                                    "",
                                    "const speedup = (coldTime / warmTime).toFixed(2);",
                                    "const improvement = (((coldTime - warmTime) / coldTime) * 100).toFixed(2);",
                                    "",
                                    "console.log('Warm query time:', warmTime + 'ms');",
                                    "console.log('Speedup:', speedup + 'x faster');",
                                    "console.log('Improvement:', improvement + '%');",
                                    "",
                                    "pm.test('Performance improvement is significant', function() {",
                                    "    pm.expect(parseFloat(speedup)).to.be.above(1.5);",
                                    "});"
                                ],
                                "type": "text/javascript"
                            }
                        }
